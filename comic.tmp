        var currImage = 0;
        var start = location.href.indexOf('#');
        if (start > -1)
        {
            start++;
            currImage = location.href.substr(start, location.href.length - start) - 1;                
            if (currImage == -1)
                currImage = 0;
            if (!isNumber(currImage))
                currImage = 0;

            SetImage();
        }

        PreloadImage();

        $("#aCurr").focus().keyup(function(event){
            if (event.keyCode == '37')                
                Previous();
            else if (event.keyCode == '39')
                Next();                
        });

        $('#imgCurrent').live("click", function () {
            Next();
        });

        $('#btnNext').click(function () {
            Next();
        });

        $('#btnPrevious').click(function () {
            Previous();
        });

        $('#selectPage').change(function () {
            currImage = parseInt($(this).val());
            SetImage();
            PreloadImage();
            if ($('#divMsg').css('display') == 'block') {
                $('#divMsg').css('display', 'none');
            }
        });

        $('#selectEpisode').change(function() {
            location.href = 'http://readcomiconline.to/Comic/The-Maxx-Maxximized/' + $(this).val();
        });

        $('#selectReadType').change(function() {
            window.location = 'http://readcomiconline.to/Comic/The-Maxx-Maxximized/Issue-1?id=46749&readType=' + $(this).val();
        });

        $('#selectQuality').change(function() {
            window.location = 'http://readcomiconline.to/Comic/The-Maxx-Maxximized/Issue-1?id=46749&quality=' + $(this).val();
        });

        function Next() {
            if (lstImages.length > currImage + 1) {
                currImage++;
                SetImage();
                PreloadImage();
                if ($('#divMsg').css('display') == 'block') {
                    $('#divMsg').css('display', 'none');
                }
            }
            else {
                var nextOption = $('#selectEpisode').find('option:selected').next();
                if (nextOption.length) {
                    location.href = 'http://readcomiconline.to/Comic/The-Maxx-Maxximized/' + nextOption.val();
                }
                else {
                    $('#divImage').empty();
                    $('#divMsg').css('display', 'block');
                }
            }
        }

        function Previous() {
            if (currImage > 0) {
                currImage--;
                SetImage();
                if ($('#divMsg').css('display') == 'block') {
                    $('#divMsg').css('display', 'none');
                }
            }
            else {
                var previousOption = $('#selectEpisode').find('option:selected').prev();
                if (previousOption.length) {
                    location.href = 'http://readcomiconline.to/Comic/The-Maxx-Maxximized/' + previousOption.val();
                }
                else {
                    alert('This is first Issue!');
                }
            }                
        }

        function SetImage() {
            location.href = "/Comic/The-Maxx-Maxximized/Issue-1" + "#" + (currImage + 1);
            $('#imgCurrent').attr('src', lstImages[currImage]);
            $('#selectPage').val(currImage);
            document.getElementById('aCurr').scrollIntoView(true);
            $("#aCurr").focus();
        }

        function PreloadImage()
        {
            var preImage;
            if (lstImages.length > currImage + 1)                
                preImage = $('<img />').attr('src', lstImages[currImage + 1]);
            if (lstImages.length > currImage + 2)                
                preImage = $('<img />').attr('src', lstImages[currImage + 2]);
        }

        function AddCloseButton(id) {
            var elem = $('#' + id);
            if (elem.position() != null) {
                var top = elem.position().top + elem.height();
                var left = elem.position().left;
                elem.after('<div class="divCloseBut" style="position:absolute; left:' + left + 'px; top: ' + top + 'px"><a href="#" onclick="$(\'#' + id + '\').remove();$(this).remove(); InitCloseButton(); return false;">Hide</a></div>');
            }
        }

        function InitCloseButton() {
            $('.divCloseBut').remove();
            AddCloseButton('adsIfrme6');
            AddCloseButton('adsIfrme8');
        }

        InitCloseButton();
    </script>
