function zoom(type) {
  cur_level = parseInt(getCookie('cur_level'));
  cur_level = isNaN(cur_level) ? 0 : cur_level;
  if ( type=='-' ) {
    if ( level[cur_level+1] ) {
      $('.chapter_img').css("max-width", level[cur_level+1]);
      setCookie('cur_level', cur_level+1, 100);
    }
